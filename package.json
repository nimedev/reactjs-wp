{
  "name": "reactjs-wp",
  "version": "7.0.2",
  "description": "A starting point for building web applications with ReactJS using ES6, Webpack and PostCSS",
  "scripts": {
    "build": "npm run install:dev && export NODE_ENV='production' && webpack --env.target production",
    "install:dev": "npm install --production=false",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint . --ext .js --ext .jsx",
    "start": "npm run watch",
    "stats": "export NODE_ENV='production' && webpack --env.target production --profile --json > webpack-stats.json && webpack-bundle-analyzer webpack-stats.json dist",
    "watch": "export NODE_ENV='development' && webpack-dev-server --env.target development"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nimedev/reactjs-wp.git"
  },
  "keywords": [
    "reactjs",
    "es6",
    "es2015",
    "webpack",
    "postcss"
  ],
  "author": "Nicolás Mejía Correa <nicolasmejiaco@gmail.com> (https://twitter.com/nimedev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nimedev/reactjs-wp/issues"
  },
  "homepage": "https://github.com/nimedev/reactjs-wp#readme",
  "engines": {
    "npm": ">=5.0.0",
    "node": ">=6.9 <7.0.0"
  },
  "dependencies": {
    "niduscss-base": "1.0.1",
    "niduscss-elements-form": "1.3.0",
    "niduscss-elements-link": "1.1.1",
    "niduscss-elements-typography": "1.1.2",
    "react": "15.5.4",
    "react-dom": "15.5.4"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "clean-webpack-plugin": "0.1.16",
    "eslint": "3.19.0",
    "eslint-config-nimedev-react": "5.0.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "ip": "1.1.5",
    "opener": "1.4.3",
    "postcss-calc": "6.0.0",
    "postcss-color-function": "4.0.0",
    "postcss-custom-media": "6.0.0",
    "postcss-custom-properties": "6.0.1",
    "postcss-import": "10.0.0",
    "postcss-mixins": "6.0.0",
    "postcss-nested": "2.0.2",
    "pre-commit": "1.2.2",
    "stylelint": "7.10.1",
    "stylelint-config-niduscss": "1.0.0",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-server": "2.4.5",
    "webpack-kit-nimedev": "9.0.0",
    "webpack-merge": "4.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "nimedev-base"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-mixins": {},
      "postcss-custom-media": {},
      "postcss-custom-properties": {},
      "postcss-nested": {},
      "postcss-calc": {},
      "postcss-color-function": {},
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-niduscss"
  }
}
