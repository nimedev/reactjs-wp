{
  "name": "reactjs-wp",
  "version": "1.0.0",
  "description": "A starting point for building web applications with ReactJS using ES6, Webpack and PostCSS",
  "scripts": {
    "build": "npm run constants && webpack --config webpack.config.build.js",
    "constants": "node src/constants",
    "start": "npm run watch",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint --ext=js .",
    "shrink": "npm prune && npm shrinkwrap --dev",
    "update": "npm install && npm prune --production=false && npm outdated",
    "watch": "parallelshell 'npm run watch:constants' 'webpack-dev-server'",
    "watch:constants": "onchange 'src/constants/**/*.js' -i -- npm run constants"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nimedev/reactjs-wp.git"
  },
  "keywords": [
    "reactjs",
    "es6",
    "es2015",
    "webpack",
    "postcss"
  ],
  "author": "nicolasmejiaco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nimedev/reactjs-wp/issues"
  },
  "homepage": "https://github.com/nimedev/reactjs-wp#readme",
  "engines": {
    "npm": ">=3.10.8",
    "node": ">=6.7.0 <7.0.0"
  },
  "dependencies": {
    "niduscss-base": "1.0.1",
    "niduscss-elements-form": "1.2.0",
    "niduscss-elements-link": "1.1.1",
    "niduscss-elements-typography": "1.1.2",
    "niduscss-lib-mixins": "1.3.0",
    "niduscss-utils-align": "1.3.1",
    "niduscss-utils-buttons": "1.0.1",
    "niduscss-utils-color": "1.0.1",
    "niduscss-utils-flex": "1.1.0",
    "niduscss-utils-shadow": "1.3.2",
    "niduscss-utils-state": "1.1.1",
    "niduscss-utils-text": "1.3.2",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  },
  "devDependencies": {
    "autoprefixer": "6.5.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "clean-webpack-plugin": "0.1.11",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "eslint": "3.7.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-react": "6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "ip": "1.1.3",
    "json-loader": "0.5.4",
    "onchange": "3.0.2",
    "parallelshell": "2.0.0",
    "postcss-calc": "5.3.1",
    "postcss-color-function": "2.0.1",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "postcss-mixins": "5.2.0",
    "postcss-nested": "1.0.0",
    "pre-commit": "1.1.3",
    "style-loader": "0.13.1",
    "stylelint": "7.3.1",
    "stylelint-config-nimedev": "7.0.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-validator": "2.2.9"
  }
}
